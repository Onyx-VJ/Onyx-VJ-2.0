<flex-config>

	<!-- Specifies the minimum player version that will run the compiled SWF. -->
	<target-player>11.0.0</target-player>

	<!-- Specifies the version of the compiled SWF -->
	<swf-version>13</swf-version>

	<compiler>
		 <define>
			<name>CONFIG::DEBUG</name>
			<value>true</value>
		 </define>
		 <define>
			<name>CONFIG::RELEASE</name>
			<value>false</value>
		 </define>
		 <define>
			<name>CONFIG::WEB</name>
			<value>false</value>
		 </define>
		 <define>
			<name>CONFIG::AIR</name>
			<value>true</value>
		 </define>

		<keep-as3-metadata>
			 <name>Parameter</name>
			 <name>PluginInfo</name>
		</keep-as3-metadata>

		<!-- Turn on generation of accessible SWFs. -->
		<accessible>false</accessible>

		<!--  DEBUG -->
		<debug>true</debug>

	</compiler>

	<!-- Metadata added to SWFs via the SWF Metadata tag. -->
	<metadata>
		<title>Onyx-VJ</title>
		<description>http://www.onyx-vj.com/</description>
		<language>EN</language>
	</metadata>

</flex-config>
